{
    "name": "Less Ads",
    "version": "0.0.8",
    "description": "Reduce Ads for many sites",
    "manifest_version": 3,
    "author": "Simin Cao",
    "content_scripts": [{
        "css": ["style.css"],
        "matches": ["https://*/*"]
    },
    {
        "css": ["6park.css"],
        "matches": ["https://*.6park.com/*",
                    "https://*.6parknews.com/*"]
    },
    {
        "css": ["willhaben.css"],
        "matches": ["https://*.willhaben.at/*"]
    },
    {
        "css":["oe24.css"],
        "matches":["https://*.oe24.at/*"]
    }],
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}